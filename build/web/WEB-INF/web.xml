<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TableGenerator</servlet-name>
        <servlet-class>com.TableGenerator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>com.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HitCounter</servlet-name>
        <servlet-class>com.HitCounter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TempServlet</servlet-name>
        <servlet-class>TempServlet.TempServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TableGenerator</servlet-name>
        <url-pattern>/tablegenerator.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/signup.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HitCounter</servlet-name>
        <url-pattern>/hitcounter.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TempServlet</servlet-name>
        <url-pattern>/tempservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
</web-app>
